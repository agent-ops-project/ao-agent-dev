{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "aco-config",
      "type": "debugpy",
      "request": "launch",
      "module": "agent_copilot.commands.config.config",
      "args": ["--config-file", "./test.yaml"],
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "aco-launch -m <script.py> [<arg>]",
      "type": "debugpy",
      "request": "launch",
      "module": "agent_copilot.commands.launch",
      "args": ["-m", "./example_workflows/debug_examples/openai_add_numbers"],
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/src/user_interface"
      ],
      "outFiles": [
        "${workspaceFolder}/src/user_interface/dist/**/*.js"
      ],
      "preLaunchTask": "npm: watch",
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}